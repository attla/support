{
    "name": "attla/support",
    "description": "Collection of resources to improve and extend ways to use laravel.",
    "keywords": [
        "attla",
        "laravel",
        "collection",
        "resources",
        "support"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Nicolau",
            "email": "lucas@octha.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Attla\\Support\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Attla\\Support\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
